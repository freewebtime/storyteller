{
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": [ "es2016" ],
    "outDir": "../storyscript-node/node_modules/storyscript",
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "out"
  ]
}