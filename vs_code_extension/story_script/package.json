{
  "name": "storyscript",
  "displayName": "story-script-extension",
  "description": "language for making stories",
  "author": "Jack Sea",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "freewebtime",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freewebtime/storyteller.git"
  },
  "scripts": {
    "postinstall": "cd server && npm install && cd ../client && npm install && cd ..",
    "compile": "tsc -p client/tsconfig.json && cd server && npm run installServer && cd .. && tsc -p server/tsconfig.json",
    "compile:client": "tsc -p client/tsconfig.json",
    "compile:server": "cd server && npm run installServer && cd .. && tsc -p server/tsconfig.json",
    "compile:storyscript": "tsc -p storyscript/tsconfig.json",
    "postcompile": "node buildTasks/test.js ../storyscript/out ../client/src/node_modules/storyscript",
    "watch:client": "tsc -w -p client/tsconfig.json",
    "watch:server": "cd server && npm run installServer && cd .. && tsc -w -p server/tsconfig.json",
    "watch:storyscript": "tsc -w -p storyscript/tsconfig.json"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^6.0.88",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "regexpu": "^4.1.3",
    "typescript": "^2.5.2"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/freewebtime/storyteller/issues"
  },
  "homepage": "https://github.com/freewebtime/storyteller#readme",
  "main": "index.js",
  "keywords": [
    "storyscript",
    "compiler",
    "storytelling",
    "programming",
    "language"
  ]
}
